{
  "name": "daisy-vue",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/daisy-vue.js",
  "module": "dist/daisy-vue.es.js",
  "umd": "dist/daisy-vue.umd.js",
  "style": "dist/daisy-vue.css",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist & rollup -c",
    "dev": "rm -rf dist & rollup -cw & yarn docs & node bin/dev.js",
    "docs": "vitepress dev docs",
    "serve": "ENV=dev node bin/deploy.js && vitepress serve docs",
    "lint": "yarn eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "catalog": "node bin/gen-catalog.js",
    "new": "node bin/new-component.js",
    "deploy": "node bin/deploy.js",
    "docs:build": "vitepress build docs"
  },
  "peerDependencies": {
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autoprefixer": "^10.4.1",
    "browser-sync": "^2.27.4",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "2.4.1",
    "rollup": "^2.52.8",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "3",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.43.2",
    "tailwindcss": "^3.0.10",
    "typescript": "^4.4.3",
    "vitepress": "^0.19.1",
    "vue": "^3.2.19",
    "vue-eslint-parser": "^7.11.0",
    "watch": "^1.0.2",
    "write": "^2.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "async-validator": "^4.0.3",
    "codesandbox": "^2.2.3",
    "daisyui": "^1.21.0",
    "diacritics": "^1.3.0",
    "mitt": "^3.0.0",
    "uuid": "^8.3.2",
    "vuelidate": "^0.7.6"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,yaml,yml,md,html}": "prettier --write"
  },
  "repository": "https://github.com/qiuquanwu/daisy-vue",
  "author": "qiuquanwu",
  "files": [
    "/dist/*"
  ]
}
